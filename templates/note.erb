<% content_for :some_key do %>
---
layout: note
<% if defined? category %><% if category.is_a? Array %><%= "tags: #{category.join(", ")}\n"  %><% else %><%= "tags: #{category}\n" %><% end %><% end %>
<%= "permalink: #{slug}\n" if defined? slug %>
date: <%= published %>
---
<%= "![](#{photo})\n\n" if defined? photo %><%= content %>
<% end %>
